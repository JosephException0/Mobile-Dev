<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp">

    <LinearLayout
        android:id="@+id/devArrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        tools:ignore="MissingConstraints">
    <Button
        android:id="@+id/setting_close_button"
        android:layout_marginTop="5dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/transparent"
        android:backgroundTint="@android:color/transparent"
        android:drawableStart="@drawable/arrow_back"/>


    <TextView
        android:id="@+id/textViewTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp"
        android:text="DEVELOPER INFO"
        android:textSize="30sp"
        android:letterSpacing=".2"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/devArrow" />
    </LinearLayout>

    <View
        android:id="@+id/viewCircle1"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/profile_dev"
        app:layout_constraintTop_toBottomOf="@+id/devArrow"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewName1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:text="Joseph Victor P."
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/viewCircle1"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewCompany1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:text="Novabos"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/textViewName1"
        app:layout_constraintStart_toStartOf="parent" />

    <FrameLayout
        android:id="@+id/frameLayout1"
        android:layout_width="210dp"
        android:layout_height="177dp"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@+id/devArrow"
        app:layout_constraintStart_toEndOf="@id/viewCircle1">

        <View
            android:layout_width="210dp"
            android:layout_height="177dp"
            android:background="@drawable/rounded_rectangle" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="4dp"
                android:layout_height="4dp"
                android:layout_marginTop="13dp"
                android:layout_marginLeft="5dp"
                android:src="@drawable/baseline_brightness_1_24" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:text="Bachelor of Science in Computer Science"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="4dp"
                android:layout_height="4dp"
                android:layout_marginTop="45dp"
                android:layout_marginLeft="5dp"
                android:src="@drawable/baseline_brightness_1_24" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="40dp"
                android:text="github.com/JosephException0"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:textStyle="bold" />
        </LinearLayout>


    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="210dp"
        android:layout_height="177dp"
        android:layout_marginTop="127dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@id/frameLayout1"
        app:layout_constraintStart_toStartOf="parent">

        <View
            android:layout_width="210dp"
            android:layout_height="177dp"
            android:background="@drawable/rounded_rectangle" />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="4dp"
                android:layout_height="4dp"
                android:layout_marginTop="13dp"
                android:layout_marginLeft="5dp"
                android:src="@drawable/baseline_brightness_1_24" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:text="Bachelor of Science in Computer Science"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="4dp"
                android:layout_height="4dp"
                android:layout_marginTop="45dp"
                android:layout_marginLeft="5dp"
                android:src="@drawable/baseline_brightness_1_24" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="40dp"
                android:text="github.com/Fishdips11"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:textStyle="bold" />
        </LinearLayout>

    </FrameLayout>

    <View
        android:id="@+id/viewCircle2"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="127dp"
        android:background="@drawable/profile_deve"
        app:layout_constraintTop_toBottomOf="@id/frameLayout1"
        app:layout_constraintStart_toEndOf="@id/frameLayout2" />

    <TextView
        android:id="@+id/textViewName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="270dp"
        android:text="Jay - Z R."
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/viewCircle2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewCompany2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="280dp"
        android:text="Olimba"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/textViewName2"
        app:layout_constraintStart_toStartOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayoutButtons"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="80dp"
        app:layout_constraintTop_toBottomOf="@id/frameLayout2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/baseline_arrow_back_ios_24" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/baseline_arrow_forward_ios_24" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>